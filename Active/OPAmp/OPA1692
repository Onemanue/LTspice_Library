*
.SUBCKT CLAW_SRC_OPA1692 VC+ VC- IOUT+ IOUT- PARAMS: GAIN=1 IPOS=0.16E1 INEG=-0.16E1
G1 IOUT+ IOUT- VALUE={LIMIT(GAIN*V(VC+,VC-),INEG,IPOS)}
.ENDS  CLAW_SRC_OPA1692
*
.SUBCKT CL_SRC_OPA1692 VC+ VC- IOUT+ IOUT- PARAMS: GAIN=1 IPOS=0.32E1 INEG=-0.32E1
G1 IOUT+ IOUT- VALUE={LIMIT(GAIN*V(VC+,VC-),INEG,IPOS)}
.ENDS  CL_SRC_OPA1692
*
.SUBCKT ESD_BB_OPA1692 ESDN ESDP
.MODEL ESD_SW VSWITCH(RON=50 ROFF=1E12 VON=700E-3 VOFF=650E-3)
S1 ESDN ESDP ESDN ESDP ESD_SW
S2 ESDP ESDN ESDP ESDN ESD_SW
.ENDS  ESD_BB_OPA1692
*
.SUBCKT ESD_IN_OPA1692 ESDN ESDP VCC VEE
.MODEL ESD_SW VSWITCH(RON=50 ROFF=1E12 VON=500E-3 VOFF=450E-3)
S1 ESDN VCC ESDN VCC ESD_SW
S2 ESDP VCC ESDP VCC ESD_SW
S3 VEE ESDN VEE ESDN ESD_SW
S4 VEE ESDP VEE ESDP ESD_SW
.ENDS  ESD_IN_OPA1692
*
.SUBCKT ESD_OUT_OPA1692 OUT VCC VEE
.MODEL ESD_SW VSWITCH(RON=50 ROFF=1E12 VON=500E-3 VOFF=450E-3)
S1 OUT VCC OUT VCC ESD_SW
S2 VEE OUT VEE OUT ESD_SW
.ENDS  ESD_OUT_OPA1692
*
.SUBCKT FEMT_OPA1692 1 2 PARAMS: FLWF=0.1 GLFF=1.645 RNVF=165171
.MODEL DVNF D KF={PWR(FLWF,0.5)/1E11} IS=1.0E-16
I1 0 7 10E-3
I2 0 8 10E-3
D1 7 0 DVNF
D2 8 0 DVNF
E1 3 6 7 8 {GLFF}
R1 3 0 1E9
R2 3 0 1E9
R3 3 6 1E9
E2 6 4 5 0 10
R4 5 0 {RNVF}
R5 5 0 {RNVF}
R6 3 4 1E9
R7 4 0 1E9
G1 1 2 3 4 1E-6
.ENDS  FEMT_OPA1692
*
.SUBCKT GR_SRC_OPA1692 VC+ VC- IOUT+ IOUT- PARAMS: GAIN=1 IPOS=60E1 INEG=-60E1
G1 IOUT+ IOUT- VALUE={LIMIT(GAIN*V(VC+,VC-),INEG,IPOS)}
.ENDS  GR_SRC_OPA1692
*
.SUBCKT IQ_SRC_OPA1692 VC+ VC- IOUT+ IOUT- PARAMS: GAIN=1E-3
G1 IOUT+ IOUT- VALUE={IF( (V(VC+,VC-)<=0),0,GAIN*V(VC+,VC-) )}
.ENDS  IQ_SRC_OPA1692
*
.SUBCKT OL_SENSE_OPA1692 COM SW+ OLN OLP
GSW+ COM SW+ VALUE = {IF((V(OLN,COM)>10E-3 | V(OLP,COM)>10E-3),1,0)}
.ENDS  OL_SENSE_OPA1692
*
.SUBCKT SW_OL_OPA1692 SW_OL_OPA1692 MID CAP_L CAP_R
.MODEL OL_SW VSWITCH(RON=1E-3 ROFF=1E12 VON=900E-3 VOFF=800E-3)
S1 CAP_L CAP_R SW_OL_OPA1692 MID OL_SW
.ENDS  SW_OL_OPA1692
*
.SUBCKT VCM_CLAMP_OPA1692 VIN+ VIN- IOUT- IOUT+ VP+ VP- PARAMS: GAIN=1
G1 IOUT+ IOUT- VALUE={LIMIT(GAIN*V(VIN+,VIN-),V(VP-,VIN-), V(VP+,VIN-))}
.ENDS  VCM_CLAMP_OPA1692
*
.SUBCKT VNSE_OPA1692 1 2 PARAMS: FLW=0.1 GLF=1.016E-2 RNV=23.1014
.MODEL DVN D KF={PWR(FLW,0.5)/1E11} IS=1.0E-16
I1 0 7 10E-3
I2 0 8 10E-3
D1 7 0 DVN
D2 8 0 DVN
E1 3 6 7 8 {GLF}
R1 3 0 1E9
R2 3 0 1E9
R3 3 6 1E9
E2 6 4 5 0 10
R4 5 0 {RNV}
R5 5 0 {RNV}
R6 3 4 1E9
R7 4 0 1E9
E3 1 2 3 4 1
.ENDS  VNSE_OPA1692
*
.SUBCKT VOS_DRIFT_OPA1692 VOS+ VOS- PARAMS: DC=2.219E-04 POL=1 DRIFT=5.00E-07
E1 VOS+ VOS- VALUE={DC+POL*DRIFT*(TEMP-27)}
.ENDS  VOS_DRIFT_OPA1692
*
.SUBCKT ZO_SRC_OPA1692 VC+ VC- IOUT+ IOUT- PARAMS: GAIN=8000 IPOS=10E5 INEG=-10E5
G1 IOUT+ IOUT- VALUE={LIMIT(GAIN*V(VC+,VC-),INEG,IPOS)}
.ENDS  ZO_SRC_OPA1692

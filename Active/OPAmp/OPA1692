.SUBCKT BLOCK_DC_H3_OPA1692 1 2 3 4  
H_H3         3 4 VH_H3 -1
VH_H3         1 2 0V
.ENDS  BLOCK_DC_H3_OPA1692
*
.SUBCKT BLOCK_DC_S1_OPA1692 1 2 3 4  
S_S1         3 4 1 2 _S1
RS_S1         1 2 1G
.MODEL         _S1 VSWITCH ROFF=1E9 RON=10M VOFF=0.0V VON=10MV
.ENDS  BLOCK_DC_S1_OPA1692
*
.SUBCKT BLOCK_DC_S2_OPA1692 1 2 3 4  
S_S2         3 4 1 2 _S2
RS_S2         1 2 1G
.MODEL         _S2 VSWITCH ROFF=1E9 RON=10M VOFF=0.0V VON=10MV
.ENDS  BLOCK_DC_S2_OPA1692
*
.SUBCKT AOL_1_OPA1692 VC+ VC- IOUT+ IOUT- PARAMS: GAIN=1E-4 IPOS=.5 INEG=-.5
G1 IOUT+ IOUT- VALUE={LIMIT(GAIN*V(VC+,VC-),INEG,IPOS)}
.ENDS  AOL_1_OPA1692
*
.SUBCKT AOL_2_OPA1692 VC+ VC- IOUT+ IOUT- PARAMS: GAIN=0.264842 IPOS=30.515 INEG=-30.515
G1 IOUT+ IOUT- VALUE={LIMIT(GAIN*V(VC+,VC-),INEG,IPOS)}
.ENDS  AOL_2_OPA1692
*
.SUBCKT CLAMP_AMP_HI_OPA1692 VC+ VC- VIN COM VO+ VO- PARAMS: G=10
GVO+ COM VO+ VALUE = {IF(V(VIN,COM)>V(VC+,COM),((V(VIN,COM)-V(VC+,COM))*G),0)}
GVO- COM VO- VALUE = {IF(V(VIN,COM)<V(VC-,COM),((V(VC-,COM)-V(VIN,COM))*G),0)}
.ENDS  CLAMP_AMP_HI_OPA1692
*
.SUBCKT CLAMP_AMP_LO_OPA1692 VC+ VC- VIN COM VO+ VO- PARAMS: G=1
GVO+ COM VO+ VALUE = {IF(V(VIN,COM)>V(VC+,COM),((V(VIN,COM)-V(VC+,COM))*G),0)}
GVO- COM VO- VALUE = {IF(V(VIN,COM)<V(VC-,COM),((V(VC-,COM)-V(VIN,COM))*G),0)}
.ENDS  CLAMP_AMP_LO_OPA1692
*
.SUBCKT CLAWN_OPA1692 VC+ VC- IOUT+ IOUT-
G1 IOUT+ IOUT- TABLE {(V(VC+,VC-))} =
+(0,        2.05E-04)
+(13.8574,  2.56E-04)
+(22.731,   3.08E-04)
+(30.9968,  4.62E-04)
+(40.9643,  5.13E-04)
+(45.3404,  6.15E-04)
+(50,       8.72E-04)
+(51,       1.80E-02)
.ENDS  CLAWN_OPA1692
*
.SUBCKT CLAWP_OPA1692 VC+ VC- IOUT+ IOUT-
G1 IOUT+ IOUT- TABLE {(V(VC+,VC-))} =
+(0,        2.05E-04)
+(23.1654,  2.56E-04)
+(29.5294,  3.59E-04)
+(38.8308,  5.13E-04)
+(44.8268,  5.64E-04)
